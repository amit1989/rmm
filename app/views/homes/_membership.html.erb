<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Designation</th>
    <th>Membershipno</th>
    <th>Contactno</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
<%@users = User.all%>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.designation %></td>
      <td><%= user.membershipno %></td>
      <td><%= user.contactno %></td>
      <td><%= user.address %></td>
      <td><%= link_to 'Show', user %></td>
      <td><%= link_to 'Edit', edit_user_path(user) %></td>
      <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
    </tr>
  <% end %>
</table>

<br />
